{
  "name": "Deals summary app",
  "description": "This app demonstrates basic handshake between React frontend code and serverless backend for a card.",
  "scopes": [
    "crm.objects.contacts.read",
    "crm.objects.contacts.write",
    "crm.objects.deals.read",
    "crm.objects.deals.write"
  ],
  "public": false,
  "extensions": {
    "crm": {
      "cards": [
        {
          "file": "extensions/deals-summary-card.json"
        }
      ]
    }
  }
}
